{
  "name": "green-screen-remover",
  "version": "1.0.0",
  "description": "Green screen removal app using Electron, React, and FFmpeg",
  "main": "public/electron.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron-dev": "cross-env ELECTRON_IS_DEV=1 electron .",
    "electron-pack": "electron-builder",
    "preelectron-pack": "npm run build",
    "dev:electron": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && cross-env ELECTRON_IS_DEV=1 electron .\""
  },
  "keywords": [
    "electron",
    "react",
    "green-screen",
    "chroma-key",
    "ffmpeg"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "ffmpeg-static": "^5.2.0",
    "electron-is-dev": "^2.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^27.0.0",
    "electron-builder": "^24.6.4",
    "vite": "^5.0.8",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0",
    "cross-env": "^7.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
